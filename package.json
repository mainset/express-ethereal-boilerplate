    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "kysely": "^0.27.5",
    "kysely-ctl": "^0.11.1",
    "nodemon": "^3.1.9",
    "pg": "^8.13.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
